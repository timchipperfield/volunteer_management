<!DOCTYPE html>
<html>
  <head>
    <title>VolunteerManagement</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <!-- navigation bar -->
    <%= render "navbar" %>

    <!-- notices and alerts -->
    <%= render "alerts" %>

    <!-- yields to further views -->
    <%= yield %>
    
  </body>
</html>
